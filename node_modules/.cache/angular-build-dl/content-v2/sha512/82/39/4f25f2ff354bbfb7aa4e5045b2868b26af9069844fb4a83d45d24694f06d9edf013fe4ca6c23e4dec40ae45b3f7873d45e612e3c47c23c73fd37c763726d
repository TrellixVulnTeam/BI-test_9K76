n               private scanModule: ScanModule,\n               private appService: AppService,\n               private camera: Camera) {}\n\n\n  \n  public showScanBtn:boolean = true\n  public showTakePic:boolean = false\n  private basicUrl:string = ''\n  public status:string = ''\n  public uploadImageName:string = ''\n  public username:string = ''\n  public password:string = ''\n  ngOnInit() {\n    console.log('on init')\n  }\n  scan() {\n    this.qrScanner.prepare()\n    .then((status: QRScannerStatus) => {\n       if (status.authorized) {\n         // start scanning, from ionic document\n         let scanSub = this.qrScanner.scan().subscribe((text: any) => {\n           if (text && typeof text.result === 'string') {\n             const tempArr = text.result.split(';')\n             this.basicUrl = 'https:' + tempArr[0].split(':')[2]\n             this.username = tempArr[1].split(':')[1]\n             this.password = tempArr[2].split(':')[1]\n             this.appService.setUrl(this.basicUrl)\n             this.scanModule.checkStatus((res: any) => { // call status api\n              this.status = res.status\n              if (res.status === 'ok') {\n                this.showScanBtn = false // switch show btn\n                this.showTakePic = true\n              } else {\n                this.showTakePic = false\n              }\n             })\n             this.qrScanner.hide(); // hide camera preview\n             scanSub.unsubscribe(); // stop scanning\n           }\n         });\n  \n       } else if (status.denied) {\n         // camera permission was permanently denied\n         // you must use QRScanner.openSettings() method to guide the user to the settings page\n         // then they can grant the permission from there\n       } else {\n         // permission was denied, but not permanently. You can ask for permission again at a later time.\n       }\n    })\n    .catch((e: any) => console.log('Error is', e));\n  \n  }\n\n  checkStatus () { // check status function\n    this.scanModule.checkStatus((res: any) => {\n      this.showScanBtn = false // switch show btn\n      this.status = res.status\n      if (res.status === 'ok') {\n        this.showTakePic = true\n      } else {\n        this.showTakePic = false\n      }\n     })\n  }\n\n  takePic () { // call camera\n    const options: CameraOptions = {\n      quality: 50,\n      destinationType: this.camera.DestinationType.FILE_URI,\n      encodingType: this.camera.EncodingType.JPEG,\n      mediaType: this.camera.MediaType.PICTURE\n    }\n    this.camera.getPicture(options).then((imageData) => {\n      let base64Image = `data:image/jpeg;base64,${imageData}`;\n      const url = `${this.basicUrl}/api/v1.0/ranking`\n      const param = {\n        \"picture\": base64Image\n      }\n      const httpOptions = {\n        headers: new HttpHeaders({\n          'Content-type': 'application/json;charset=utf-8',\n          // set auth, but seems doesn't work on browser\n          'Authorization': 'Basic ' + btoa(`${this.username}:${this.password}`)\n        })\n      }\n      // no idea how to limit pic size on ionic\n      this.scanModule.uploadImage(param, httpOptions, (res: any) => {\n        if (res) {\n          this.uploadImageName = `${this.basicUrl}/api/v1.0/image/${res.file}`\n        }\n      })\n     }, (err) => {\n     })\n     \n  }\n\n}\n","import { Injectable } from '@angular/core'\n\n@Injectable()\n\nexport class AppService{\n\npublic baseUrl: string;\n\nconstructor() {\n    this.baseUrl = ''\n}\n\n    setUrl (val: string) {\n        this.baseUrl = val\n    }\n\n    getUrl () {\n        return this.baseUrl\n    }\n\n}","import {Injectable, NgModule} from '@angular/core'\nimport {HttpClient, HttpParams } from '@angular/common/http'\nimport { AppService } from \"../service/global\"\n\n@Injectable()\n@NgModule({\n  providers: [AppService]\n})\nexport class httpRequest {\n  public http: any\n\n  constructor(Http: HttpClient, private appService: AppService) {\n    this.http = Http\n  }\n\n  public get (url: string, params?: Object, callback?: Function) {\n    const baseUrl = this.appService.getUrl()\n    let httpParams = new HttpParams()\n    const vm = this\n    if (params) {\n      for (const key in params) {\n        if (params[key] === false || params[key]) {\n          httpParams = httpParams.set(key, params[key])\n        }\n      }\n    }\n    vm.http.get(baseUrl + url, {params: httpParams})\n      .subscribe((data: any) => {\n        callback(data)\n      })\n  }\n\n  public post(url: string, data?: Object, options?: Object, callback?: Function) {\n    const baseUrl = this.appService.getUrl()\n    const vm = this\n    vm.http.post(baseUrl + url, data, options)\n      .subscribe((res: any) => {\n        callback(res)\n      })\n  }\n}\n","import {Injectable} from '@angular/core'\nimport { httpRequest } from '../service/http'\n\n@Injectable()\nexport class ScanModule {\n  public httpRequest: any\n\n  constructor(httpRequest: httpRequest) {\n    this.httpRequest = httpRequest\n  }\n\n  public checkStatus (callback: Function) {\n    this.httpRequest.get('api/v1.0/status', {}, (res: any) => {\n        callback(res)\n    })\n  }\n\n  public uploadImage (param: Object, httpOptions: Object, callback: Function) {\n      this.httpRequest.post('api/v1.0/image', param, httpOptions, (res: any) => {\n        callback(res)\n      })\n  }\n}\n","export default \"#container {\\n  text-align: center;\\n  position: absolute;\\n  left: 0;\\n  right: 0;\\n  top: 50%;\\n  transform: translateY(-50%);\\n}\\n\\n#container strong {\\n  font-size: 20px;\\n  line-height: 26px;\\n}\\n\\n#container p {\\n  font-size: 16px;\\n  line-height: 22px;\\n  color: #8c8c8c;\\n  margin: 0;\\n}\\n\\n#container a {\\n  text-decoration: none;\\n}\\n/*# sourceMappingURL=data:application/json;base64, */\";","export default \"  <ion-header *ngIf=\\\"uploadImageName && uploadImageName !== ''\\\">\\n    <ion-toolbar>\\n      <ion-title size=\\\"small\\\">Your upl